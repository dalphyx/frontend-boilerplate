{
  presets: [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  plugins: [
    "transform-runtime"
  ],
  "env": {
    "ava": {
      "presets": [
        "es2015",
        "stage-2"
      ]
    },
    "development": {
      plugins: [
        "transform-es2015-modules-commonjs",
        "transform-async-to-generator",
        "syntax-async-functions"
      ]
    }
  }
}