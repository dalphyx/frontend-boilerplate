{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate for damned frontend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "test": "cross-env BABEL_ENV=ava ava ./test --verbose",
    "dev": "cross-env NODE_ENV=development nodemon --watch client --exec \"npm run build && node\" ./build/dev-server",
    "build": "cross-env NODE_ENV=development webpack --config ./build/webpack.config",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.config"
  },
  "author": "dalphyx <wjcbmk@gmail.com>",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "ejs": "^2.5.2",
    "eslint": "^3.8.1",
    "eslint-config-dalphyx": "0.1.0",
    "eslint-loader": "^1.6.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "jsdom": "^9.8.2",
    "koa": "^2.0.0-alpha.7",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.1.2",
    "koa-webpack-middleware": "^1.0.1",
    "nodemon": "^1.11.0",
    "pokore": "^0.2.1",
    "postcss-loader": "^1.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "vue-loader": "^9.7.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.0.3",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.5",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "vue": "^2.0.3",
    "vuex": "^2.0.0"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
