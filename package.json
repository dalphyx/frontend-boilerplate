{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate for damned frontend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "test": "cross-env BABEL_ENV=test jest",
    "dev": "cross-env NODE_ENV=development npm run build && node ./build/dev-server",
    "build": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.config.prod"
  },
  "author": "dalphyx <wjcbmk@gmail.com>",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "eslint": "^4.3.0",
    "eslint-config-dalphyx": "0.2.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^20.0.4",
    "koa": "^2.3.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "koa-views": "^6.0.2",
    "koa-webpack-middleware": "^1.0.5",
    "nodemon": "^1.11.0",
    "pokore": "^0.2.1",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "vue-loader": "^13.0.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "vue": "^2.4.2",
    "vuex": "^2.3.1"
  }
}
