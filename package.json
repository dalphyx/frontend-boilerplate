{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate for damned frontend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "test": "cross-env BABEL_ENV=ava ava ./test --verbose",
    "dev": "cross-env NODE_ENV=development npm run build && node ./build/dev-server",
    "build": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.config.prod"
  },
  "author": "dalphyx <wjcbmk@gmail.com>",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.5",
    "eslint": "^3.15.0",
    "eslint-config-dalphyx": "0.2.0",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "jsdom": "^9.10.0",
    "koa": "^2.0.0-alpha.7",
    "koa-router": "^7.1.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.0",
    "koa-webpack-middleware": "^1.0.3",
    "nodemon": "^1.11.0",
    "pokore": "^0.2.1",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "vue-loader": "^10.3.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "vue": "^2.1.10",
    "vuex": "^2.1.1"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
