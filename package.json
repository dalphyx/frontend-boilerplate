{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate for damned frontend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "test": "cross-env BABEL_ENV=ava ava ./test --verbose",
    "dev": "cross-env NODE_ENV=development npm run build && node ./build/dev-server",
    "build": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.config.prod"
  },
  "author": "dalphyx <wjcbmk@gmail.com>",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "ejs": "^2.5.6",
    "eslint": "^3.19.0",
    "eslint-config-dalphyx": "0.2.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "jsdom": "^10.1.0",
    "koa": "^2.2.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^6.0.2",
    "koa-webpack-middleware": "1.0.3",
    "nodemon": "^1.11.0",
    "pokore": "^0.2.1",
    "postcss-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "vue-loader": "^12.0.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "vue": "^2.3.2",
    "vuex": "^2.3.1"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
